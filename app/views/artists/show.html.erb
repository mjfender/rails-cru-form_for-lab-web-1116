<h1><%= @artist.name %></h1>
<p> <%= @artist.bio %></p>
<h3> Songs:</h3>
<ul>
<% @artist.songs.each do |song| %>
  <li> <%= link_to song.name, song_path(song) %> </li>
<% end %>
</ul><br>

<h3> Genres:</h3>
<ul>
<% @artist.genres.each do |genre| %>
  <li> <%= link_to genre.name, genre_path(genre) %> </li>
<% end %>
</ul><br>

